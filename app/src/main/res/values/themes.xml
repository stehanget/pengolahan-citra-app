        <item name="colorPrimary">#2b2d42</item>
        <item name="colorPrimaryVariant">#2b2d42</item>
        <item name="colorOnPrimary">#edf2f4</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#8d99ae</item>
